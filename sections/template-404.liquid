<div class="w-full h-[30em] bg-white flex justify-center items-center flex-col">
    <p class="text-center text-[2em]">{{section.settings.title}}</p>
    <h1 class="text-center">{{section.settings.subtext}}</h1>
</div>




{% schema %}
{
    "name": "Template 404",
    "settings": [
              {
                "type": "text",
                "id": "title",
                "default" : "You Lost",
                "label": "404 template"
              },
              {
                "type": "text",
                "id": "subtext",
                "default" :"The page you are looking for does not exist",
                "label": "404 template"
              }
        ]
}
{% endschema %}

